#define PAUSE_TIME 1000


Pause PAUSE_TIME
Rotate Podstawa 45 Z 45

Pause PAUSE_TIME
Rotate Podstawa.Ramie1 45 Y 45

Pause PAUSE_TIME
Rotate Podstawa.Ramie1.Ramie2 45 Y 45

Pause PAUSE_TIME
Rotate Podstawa 45 Z -45

Pause PAUSE_TIME
Begin_Parallel_Actions
Move Podstawa 4 24
Rotate Podstawa 60 Z -360
Rotate Podstawa 60 X 360
End_Parallel_Actions

